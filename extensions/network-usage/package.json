{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "network-usage",
  "title": "Network Usage",
  "description": "Display network bandwith from Raycast",
  "icon": "icon.png",
  "author": "oktay",
  "categories": [
    "Security",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Network Usage",
      "description": "Display network bandwith",
      "mode": "menu-bar",
      "interval": "1m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "millify": "^5.0.0",
    "moment": "^2.29.4",
    "node-schedule": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
